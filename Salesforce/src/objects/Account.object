    <webLinks>
        <fullName>Assign_To_Me</fullName>
        <availability>online</availability>
        <description>Assign selected accounts to logged in user</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign To Me</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.Account )} ; 
var oName = &apos;Account&apos;; 
var error = false; 
var msj = &apos;&apos;;
var newRecords = [];
if( records.length &gt; 20 ) 
{ 
	error = true; 
	msj =&apos;{!$Label.ap_Errors_moreThan20records}&apos;; 
}
 
if( records.length &lt; 1 )
{ 
	error = true; 
	msj = &apos;{!$Label.ap_Errors_selectOneRecord}&apos;; 
} 

if( error ) 
{ 
	alert(msj); 
} 
else
{ 
	try {
		for (var n=0; n&lt;records.length; n++){
		
			var selAcc = new sforce.SObject(&quot;Account&quot;);
			selAcc.id = records[n];
			sforce.connection.sessionId = window.ApiUtils.getSessionId(); 
			
			selAcc.ownerId = window.UserContext.userId;
			
			newRecords.push(selAcc);
		}
		
		result = sforce.connection.update(newRecords);
		alert(&quot;Test--------&quot;+result);
		window.location.reload(); 
		if (result[0].getBoolean(&quot;success&quot;)) {
			alert(&quot;Assigned Accounts successfully.&quot;);
		} else {
			alert(&quot;failed to update Account records&quot; + result[0]);
		}
	}
	catch (e) {
		&apos;{!$Label.Apex_Exception}&apos;;
	}
	
}</url>
