<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Switch settings to allow custom automated processes to be disabled per profile</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Process_Name__c</fullName>
        <description>Name(s) of the automated process affected by the setting. Multiple processes can be entered, delimit with a semi-colon</description>
        <externalId>false</externalId>
        <inlineHelpText>Multiple processes can be entered, delimit with a semi-colon</inlineHelpText>
        <label>Process Name</label>
        <length>200</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Setting_Activated__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Set to true to deactivate the custom automated processes listed for the user/profile</description>
        <externalId>false</externalId>
        <inlineHelpText>Set to checked to deactivate the custom automated processes listed for the user/profile</inlineHelpText>
        <label>Setting Activated</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Automated Process Deactivation</label>
    <visibility>Protected</visibility>
</CustomObject>
