REGEX( TEXT( DEP_PL__c ),&quot;(?:IT|ADMIN)&quot; )

),
AND(
TEXT( PRIORVALUE( DEP_PL__c ) ) =&quot;IT&quot; ,  
 
REGEX( TEXT( DEP_PL__c ),&quot;(?:HR|ADMIN)&quot; )

))</errorConditionFormula>
